    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@next/mdx": "^15.3.1",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "eslint": "^9.17.0",
